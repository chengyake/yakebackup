<!--111111111 -->
    <LinearLayout   
        android:orientation="horizontal"  
        android:layout_width="fill_parent"  
        android:layout_height="wrap_content"  
        >

        <TextView   
            android:text="@string/addr"  
            android:gravity="center"  
            android:background="#33a1c9"  
            android:layout_width="0dp"  
            android:layout_height="fill_parent"  
            android:textSize="20dp"
            android:layout_weight="1"/>   
        <TextView   
            android:text="@string/old_v"  
            android:gravity="center"  
            android:background="#33a1c9"  
            android:layout_width="0dp"  
            android:layout_height="fill_parent"  
            android:textSize="20dp"
            android:layout_weight="1"/>
        <TextView 
            android:text="@string/new_v"  
            android:gravity="center"  
            android:background="#33a1c9"  
            android:layout_width="0dp"  
            android:layout_height="fill_parent"  
            android:textSize="20dp"
            android:layout_weight="1"/>   
    </LinearLayout>


<!--2222222222 -->
    <ListView android:layout_width="fill_parent" 
              android:layout_height="0dp" 
              android:id="@+id/pus_job_data_listview"
              android:fadingEdge="vertical"
              android:layout_weight="1"
          />

    <TextView   
        android:layout_width="fill_parent"  
        android:layout_height="2dp"  
    />  


<!--33333333333 -->
    <LinearLayout
        android:orientation="vertical"  
        android:transcriptMode="alwaysScroll" 
        android:layout_width="fill_parent"  
        android:layout_height="0dp"  
        android:layout_alignParentBottom="true"
        android:layout_weight="1"
    >

        <!--download size pagedown-->
        <LinearLayout   
            android:orientation="horizontal"  
            android:layout_width="fill_parent"  
            android:layout_height="0dp"  
            android:layout_weight="1"
            android:layout_marginTop="1dp"
            android:layout_marginBottom="1dp"
        >   
            <TextView   
                android:text="@string/inputaddr"  
                android:layout_width="0dp"  
                android:layout_height="fill_parent"  
                android:textSize="20dp"
                android:gravity="center"
                android:layout_weight="1"
                android:layout_marginRight="1dp"
                android:layout_marginLeft="1dp"
                android:background="#ffffff"  
                />
            <EditText
                android:id="@+id/pus_job_data_addr"
                android:background="#ffffff"  
                android:layout_width="0dp"  
                android:layout_height="fill_parent"  
                android:gravity="center"
                android:inputType="none" 
                android:textSize="20dp"
                android:layout_weight="2"
                android:layout_marginRight="1dp"
                android:layout_marginLeft="1dp"
            />   
            <Button
                android:id="@+id/pus_job_data_upload"
                android:layout_width="0dp" 
                android:layout_height="fill_parent" 
                android:background="#ffffff"  
                android:text="@string/upload"
                android:layout_weight="1"
                android:layout_marginRight="1dp"
                android:layout_marginLeft="1dp"
            />
        </LinearLayout>  

        <!--upload addr -->
        <LinearLayout   
            android:orientation="horizontal"  
            android:layout_width="fill_parent"  
            android:layout_height="0dp"  
            android:layout_weight="1"
            android:layout_marginTop="1dp"
            android:layout_marginBottom="1dp"
        >
            <TextView   
                android:text="@string/inputsize"  
                android:layout_width="0dp"  
                android:layout_height="fill_parent"  
                android:textSize="20dp"
                android:gravity="center"
                android:layout_weight="1"
                android:layout_marginLeft="1dp"
                android:layout_marginRight="1dp"
                android:background="#ffffff"  
                />
            <EditText
                android:id="@+id/pus_job_data_size"
