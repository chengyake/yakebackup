                android:layout_height="fill_parent" 
                android:background="#ffffff"  
                android:text="@string/download"
                android:layout_weight="1"
                android:layout_marginRight="1dp"
                android:layout_marginLeft="1dp"
            />
        </LinearLayout>  
        

        

